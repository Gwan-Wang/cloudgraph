<!DOCTYPE html>
<html>
<head>
    <!-- Standard Meta -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1.0" />

    <title><%= title %></title>

    <link rel='stylesheet' href='/css/semantic.css' />
    <link rel='stylesheet' href='/css/style.css' />

    <script src="/javascript/lib/jquery-2.1.0.js"></script>
    <script src="/javascript/lib/semantic-v0.15.2.js"></script>
    <script src="/javascript/lib/kinetic-v5.1.0.js"></script>
    <script src="/socket.io/socket.io.js"></script>
</head>
<body>

    <div id="main-menu" class="ui menu fixed transparent inverted">
        <a id="toggle-left-sidebar" class="item">
            <i class="list layout icon"></i>
        </a>
    </div>

    <div id="left-sidebar" class="ui sidebar menu vertical inverted labeled">
        <a id="close-left-sidebar" class="item">
            <div align="center">
                <i class="close icon circular inverted"></i>
            </div>
        </a>

        <a class="item">
            <i class="home icon"></i>
            Home
        </a>
        <a class="active item">
            <i class="heart icon"></i>
            Current Section
        </a>
        <a class="item">
            <i class="facebook icon"></i>
            Like us on Facebook
        </a>
        <div class="item">
            <b>More</b>
            <div class="menu">
                <a class="item">About</a>
                <a class="item">Contact Us</a>
            </div>
        </div>

        <div class="item">
            <div class="ui small icon input">
                <input type="text" placeholder="Search...">
                <i class="search icon"></i>
            </div>
        </div>
    </div>

    <div id="node-detail-sidebar" class="ui sidebar right very wide menu vertical inverted labeled">
        <a id="close-node-detail-sidebar" class="item">
            <div align="center">
                <i class="close icon circular inverted"></i>
            </div>
        </a>
        <div class="item">
            <div id="node-detail-sidebar-header" class="header" align="center">
                 Header
            </div>
        </div>

        <div class="item segment">
            <div id="node-detail-sidebar-content" class="ui animated ordered list">
                <div class="item"></div>
            </div>
        </div>
    </div>


    <div id="path-detail-sidebar" class="ui sidebar right very wide menu vertical inverted labeled">
        <a id="close-path-detail-sidebar" class="item">
            <div align="center">
                <i class="close icon circular inverted"></i>
            </div>
        </a>
        <div class="item">
            <div id="path-detail-sidebar-header" class="header" align="center">
                 Header
            </div>
        </div>

        <div class="item segment">
            <div class="ui label orange">Order</div>
            <div id="path-detail-sidebar-content" class="ui animated ordered list">
                <div class="item"></div>
            </div>
        </div>
    </div>

    <!--<div id="popup" onselectstart="return false" style="z-index:999; position:absolute"></div>-->
    <div id="canvas" class="ui segment inverted">
    </div>

    <script src="/javascript/types.js"></script>
    <script src="/javascript/ui.js"></script>
    <script src="/javascript/drawing.js"></script>
    <script src="/javascript/communion.js"></script>
</body>
</html>
